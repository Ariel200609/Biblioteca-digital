{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node ./dist/index.js",
    
    "// COMENTARIO": "--- Scripts de Desarrollo ---",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:frontend": "npm run dev --prefix src/Frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    
    "clean": "rimraf dist",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/Database/config/database.config.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/Database/config/database.config.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/Database/config/database.config.ts",
    "db:setup": "ts-node ./src/Database/config/setup.ts",
    "db:sync": "ts-node ./src/Database/config/sync.ts",
    "db:test": "ts-node ./src/Database/config/testConnection.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.9.2",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27",
    "vite": "^7.2.2"
  }
}